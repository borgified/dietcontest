<VirtualHost *:80>
	ServerAdmin webmaster@localhost

	DocumentRoot /home/jctong/dietcontest/html
	<Directory />
		Options FollowSymLinks
		AllowOverride None
	</Directory>
	<Directory /home/jctong/dietcontest/html>
		Options Indexes FollowSymLinks MultiViews
		AllowOverride None
		Order allow,deny
		allow from all
	</Directory>

	ScriptAlias /cgi-bin/ /home/jctong/dietcontest/cgi-bin/
	<Directory "/home/jctong/dietcontest/cgi-bin">
		AllowOverride None
		Options +ExecCGI -MultiViews +SymLinksIfOwnerMatch
		Order allow,deny
		Allow from all
	</Directory>
